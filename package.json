{
  "name": "abapcdsgrammar",
  "version": "0.0.1",
  "description": "Grammar for ABAP CDS",
  "main": "index.js",
  "scripts": {
    "test": "npm run antlr4ts; tsc -p ./tsconfig.test.json; node test/test.js",
    "build": "npm run clean; npm run antlr4ts; tsc -p ./tsconfig.json",
    "clean": "rm -rdf dist test .antlr",
    "publish": "npm run clean; rm -rdf node_modules; npm publish",
    "antlr4ts": "antlr4ts -visitor ./ABAPCDS.g4"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^12.12.21",
    "antlr4ts-cli": "^0.5.0-alpha.3",
    "tslint": "^5.20.1",
    "tslint-config-airbnb": "^5.11.2",
    "typescript": "^3.7.2"
  },
  "dependencies": {
    "antlr4ts": "^0.5.0-alpha.3"
  }
}
